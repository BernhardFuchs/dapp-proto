{
  "name": "dapp-proto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng build --prod && http-server ./dist/dapp-proto --ssl",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:prepare": "docker build -t nginx-server -f ./docker/Dockerfile . && docker stop dapp && docker rm dapp && docker run --name dapp -p 32700:443 -d nginx-server:latest ",
    "docker:deploy": "docker cp ./docker/nginx.conf dapp:/etc/nginx/nginx.conf && docker cp ./dist/dapp-proto/. dapp:/usr/share/nginx/html && docker cp ~/ssl/. dapp:/etc/nginx/ssl",
    "docker:reload": "docker container exec dapp nginx -s reload",
    "postinstall": "node ./scripts/crypto.patch.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/pwa": "^0.13.1",
    "@angular/router": "~7.1.0",
    "@angular/service-worker": "~7.1.0",
    "@ngxs/devtools-plugin": "^3.3.2",
    "@ngxs/store": "^3.3.2",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "rxjs": "~6.3.3",
    "web3": "^1.0.0-beta.36",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.119",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "colors": "^1.3.0",
    "csstips": "^0.3.0",
    "csx": "^9.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.15.3",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.1.6",
    "typestyle": "^2.0.1"
  }
}
